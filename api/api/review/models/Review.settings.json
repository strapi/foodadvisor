{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "reviews",
  "info": {
    "name": "review",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": "",
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "content": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "note": {
      "type": "integer",
      "max": 5,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "author": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "reviews"
    },
    "restaurant": {
      "via": "reviews",
      "model": "restaurant"
    }
  }
}
